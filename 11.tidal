do
  setcps (130/60/4)
  -- Basic techno beat
  d1 $ stack [
    s "bd(3,8)" # gain 1.2,
    s "~ cp ~ cp" # room 0.3,
    fast 2 $ s "hh*2" # gain 0.8 ]
  -- Atmospheric lpviz layer
  d2 $ every 4 (rev) 
    $ loopAt 2 
    $ chop 8 
    $ s "lpviz" # n 388 
    # gain 0.9 
    # room 0.4
  -- Bass texture from lpviz
  d3 $ every 8 (stut 3 0.5 (1/3))
    $ s "lpviz" # n 832
    # speed 0.5
    # gain (range 0 0.85 $ slow 16 sine)
    # lpf (range 200 1000 $ slow 16 sine)
  
  -- Melodic progression with flbass
  d4 $ every 8 (stut 3 0.5 (1/3))
    $ note "c4 e4 g4 a4"
    # s "flbass"
    # n "3"
    # gain (range 0 0.8 $ slow 16 tri)
    # speed 1.2
    # room 0.3

  -- Silence commands
  --d1 silence
  d2 silence
  --d3 silence
  --d4 silence
