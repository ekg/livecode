-- Polyrhythmic patterns with Euclidean rhythms
do
  hush
  setcps (126/60/4)
  -- Layered Euclidean rhythms
  d1 $ stack [
    struct "t(4,8)" $ s "808lt" # n 3 # gain 1.1,
    struct "t(3,8,2)" $ s "realclaps" # n "[3,7]",
    struct "t(7,12)" $ s "hh" # gain 0.8 # pan sine,
    struct "t(4,7)" $ s "808mt" # n (irand 4) # speed "<1 0.5 1.5 0.75>" ]
