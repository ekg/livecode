    # room 0.86 # size 0.8
    # lpf (range 800 2000 (slow 8 sine))
    # pan (range 0.3 0.7 (slow 4 sine)),
  -- Log drum bass pattern with more groove
  every 2 (slow 2)
    $ every 3 (fast 2)
    $ every 4 (# crush 8)
    $ note (scale "minor" (scramble 8 "-24 -21 -22 -24 -19 -21 -17 -22"))
