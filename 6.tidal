  every 4 (fast 2) 
    $ every 3 (# speed "1.5 0.8") 
    $ s "mars_kits_909_Friendly_2(3,8)" # n "3" # gain 1.4 # lpf 200,
  every 8 (slice 8 "3 1 4 2 6 0 7 5") 
    $ loopAt 2 $ chop 8 
